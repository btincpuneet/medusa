{
  "name": "my-backend",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "migrate:categories": "node scripts/categories.js",
    "migrate:products": "npx medusa exec scripts/product.js",
    "migrate:customers": "node scripts/customers.js",
    "migrate:inventory": "node scripts/inventory.js",
    "migrate:prices": "node scripts/prices.js",
    "migrate:promotions": "node scripts/promotions.js",
    "migrate:orders": "node scripts/orders.js",
    "migrate:all": "node scripts/run-all.js",
    "repair:customer-orders": "npx ts-node src/scripts/link-orders-to-customers.ts",
    "sync:access-mappings": "npx ts-node src/scripts/sync-access-mappings.ts",
    "repair:customer-registration": "npx ts-node src/scripts/mark-customers-registered.ts",
    "backfill:customer-addresses": "npx ts-node src/scripts/backfill-customer-addresses.ts",
    "sync:redington": "npx ts-node scripts/sync-redington-from-magento.ts",
    "migrate:redington-tables": "npx ts-node src/scripts/migrate-redington-tables.ts",
    "migrate:customer-sync": "npx ts-node src/scripts/migrate-customer-sync.ts",
    "import:product-descriptions": "npx ts-node src/scripts/import-product-descriptions.ts",
    "admin:dev": "cd src/admin && npm install && npm run dev",
    "admin:build": "cd src/admin && npm install && npm run build"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/currency": "^2.10.3",
    "@medusajs/framework": "^2.10.3",
    "@medusajs/inventory": "^2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@medusajs/modules-sdk": "^1.12.12",
    "@medusajs/pricing": "^2.10.3",
    "@medusajs/product": "^2.10.3",
    "@medusajs/promotion": "^2.10.3",
    "@medusajs/stock-location": "^2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "adm-zip": "^0.5.16",
    "argon2": "^0.44.0",
    "awilix": "^8.0.1",
    "axios": "^1.10.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.5.0",
    "mysql2": "^3.11.3",
    "pg": "^8.13.0",
    "typeorm": "^0.3.27",
    "jsonwebtoken": "^9.0.2",
    "react-data-table-component": "^7.6.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.3",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "bcryptjs": "^3.0.2",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
